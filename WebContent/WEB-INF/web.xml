<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>shopping-cart</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>LoginSrv</servlet-name>
    <servlet-class>com.shashi.srv.LoginSrv</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginSrv</servlet-name>
    <url-pattern>/LoginSrv</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddtoCart</servlet-name>
    <servlet-class>com.shashi.srv.AddtoCart</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddtoCart</servlet-name>
    <url-pattern>/AddtoCart</url-pattern>
  </servlet-mapping>

</web-app>
